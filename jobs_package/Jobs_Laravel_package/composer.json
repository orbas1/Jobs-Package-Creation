{
    "name": "jobs/package-laravel",
    "description": "Jobs module extracted from the jobi application as a reusable Laravel package.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/routing": "^10.0|^11.0",
        "illuminate/view": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "JobsLaravelPackage\\": "src/",
            "JobsLaravelPackage\\Database\\": "database/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JobsLaravelPackage\\JobsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
