{
    "name": "jobs/laravel-package",
    "description": "Jobs module extracted from the Jobi platform for reuse as a Laravel package.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Jobs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jobs\\JobsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
